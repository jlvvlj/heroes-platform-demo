    "@sentry/webpack-plugin": "^1.6.2",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "chalk": "^2.4.2",
    "copy-webpack-plugin": "^5.0.2",
    "cross-env": "^5.2.0",
    "csvtojson": "^2.0.8",
    "dotenv": "^6.2.0",
    "eslint": "^5.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "glob": "^7.1.4",
    "husky": ">=1",
    "i18n-compile": "^1.1.1",
    "lint-staged": ">=8",
    "markdown-it": "^8.4.2",
    "webfontloader": "^1.6.28"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
  },
  "browserslist": [
    "> 0.2%",
    "last 2 versions",
    "ie 11",
    "ios 8.4",
    "not dead"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}
