{
  "name": "heroes-platform-demo",
  "version": "0.1.0",
  "description": "Platform demo featuring Super Heroes with Stelace API",
  "main": "index.js",
  "author": "Stelace (https://stelace.com)",
  "license": "MIT",
  "private": false,
  "scripts": {
