    "@sentry/integrations": "^5.4.0",
    "axios": "~0.19.0",
    "bezier-easing": "^2.1.0",
    "lodash": "^4.17.11",
    "mapbox-gl": "~1.1.0",
    "mapbox-gl-vue": "^1.9.0",
    "ms": "^2.1.1",
    "p-map": "^2.1.0",
    "p-props": "^3.0.1",
    "photoswipe": "^4.1.3",
    "prismjs": "^1.16.0",
    "quasar": "~1.0.4",
    "socket.io-client": "~2.2.0",
    "stelace": "0.5.0",
    "url-parse": "^1.5.0",
    "vue-analytics": "5.17.0",
    "vue-intl": "3.1.0",
